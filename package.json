{
  "name": "remark-emoji",
  "version": "4.0.1",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "description": "Emoji transformer plugin for Remark",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    }
  },
  "scripts": {
    "eslint": "eslint .",
    "prettier": "prettier -c '**/*.js' '**/*.ts'",
    "lint": "concurrently -c auto npm:eslint npm:prettier",
    "build": "tsc --pretty -p . -d",
    "watch": "tsc -w -p .",
    "format": "prettier -w '**/*.js' '**/*.ts'",
    "mocha": "mocha --color test.js",
    "test": "concurrently -m 1 -c auto npm:build npm:mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhysd/remark-emoji.git"
  },
  "keywords": [
    "markdown",
    "emoji",
    "remark",
    "plugin"
  ],
  "types": "index.d.ts",
  "files": [
    "types/index.d.ts",
    "index.js"
  ],
  "author": "rhysd <lin90162@yahoo.co.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhysd/remark-emoji/issues"
  },
  "homepage": "https://github.com/rhysd/remark-emoji#readme",
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.11",
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-security": "^1.7.1",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-github": "^12.0.0",
    "remark-html": "^16.0.1",
    "remark-rehype": "^11.1.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@types/mdast": "^4.0.2",
    "emoticon": "^4.0.1",
    "mdast-util-find-and-replace": "^3.0.1",
    "node-emoji": "^2.1.0",
    "unified": "^11.0.4"
  }
}
